flatIdent = ~keyword identFirst identRest*
identFirst = "a" .. "z" | "A" .. "Z"
identRest = ~separator any

keyword = "inputs" | "outputs" | "contains" | "connections" | "send" | "abort"
bracket = "(" | ")" | "<" | ">" | "[" | "]"
misc = "_" | ":"
slash = "/"
dotslash = "./"
dot = "."
separator = misc | bracket | dotslash | dot | slash | whitespace
whitespace = " " | "\t" | "\n" | "," | ";"

space := whitespace
